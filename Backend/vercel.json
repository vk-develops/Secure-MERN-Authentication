{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" },
        {
            "src": "../Frontend/dist/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}
